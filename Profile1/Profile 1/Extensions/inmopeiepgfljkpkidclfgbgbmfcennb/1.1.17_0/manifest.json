{
   "action": {
      "default_icon": "logo.png"
   },
   "author": "Solaiman Kmail",
   "background": {
      "service_worker": "/background.js"
   },
   "description": "Show multiple screens once, Responsive design tester",
   "icons": {
      "128": "logo.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Responsive Viewer",
   "permissions": [ "storage", "activeTab", "webNavigation", "webRequest", "declarativeNetRequest", "scripting" ],
   "sandbox": {
      "pages": [ "sandbox.html" ]
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.17",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "device-list/*", "*.css", "sandbox.html" ]
   } ]
}
